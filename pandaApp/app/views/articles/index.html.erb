<h1> List of Saved Articles </h1>

<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Url</th>
			<th>User_id</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @articles.each do |article| %>
		<tr>
			<td><%= article.title %></td>
			<td><%= article.url %></td>
			<td><%= article.user_id %></td>
			<td><%= link_to 'Destroy', [article.user, article], method: :delete %></td>
		</tr>
		<% end %>
	</tbody>
</table>